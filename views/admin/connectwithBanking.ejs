<html>
    <head><% include ./partials/head %></head>
    <body>
        <div class="container">
                <label>Connect with banking.... </label>
                <form id="bankform" method="POST" action="/admin/banking/callbackFrom_addFunds">
                <div class="form-group">
                    <label>Merchant Name.</label>
                    <input type="text" name="domain" id="domainname"  value="<%=domainname%>"/>
                </div>
                <div class="form-group">
                    <label>loginaccount</label>
                    <input type="text" readonly name="loginaccount" id="loginaccount"  value="<%=loginaccount%>"/>
                </div>
                <div class="form-group">
                    <label>Service ID</label>
                    <input type="text" readonly name="service_id" id="service_id"  value="<%=service_id%>"/>
                </div>
                
                <div class="form-group">
                        <label>Amount:</label>
                        <input type="number" required class="col-3" name="amount"/>
                    </div>
                <div class="form-group">
                    <button type="submit" id="btnbanksubmit" >Submit</button>
                    
                </div>
                </form>
        </div>
    </body>
</html>
