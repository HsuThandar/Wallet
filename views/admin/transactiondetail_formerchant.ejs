<table class="table table-bordered">
    <tbody>
        <tr>
            <td class="td-label">Transaction ID</td>
            <td><%= transaction.mer_tran_log_v_id %></td>
        </tr>
        <tr>
            <td class="td-label">Date</td>
            <td><%= transaction.mer_tran_date %></td>
        </tr>
        <tr>
            <td class="td-label">Transaction Type</td>
            <td><%= transaction.mer_tran_type %></td>
        </tr>
        <tr>
            <td class="td-label">Customer</td>
            <td><%= transaction.logianccount ? transaction.logianccount : transaction.service_customer %>(<%= transaction.service_name %>)</td>
        </tr>
        <tr>
            <td class="td-label">Transaction Status</td>
            <td><%= transaction.mer_tran_status %></td>
        </tr>
        <tr>
            <td class="td-label">Transaction Amount</td>
            <td><%= transaction.amount %></td>
        </tr>
        
    </tbody>
</table>
